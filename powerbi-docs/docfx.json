{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/*.md"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/power-bi/breadcrumb/toc.json",
      "uhfHeaderId": "MSDocsHeader-MSPowerBI",
      "contributors_to_exclude": ["openpublishingbuild","sudeepku"],      
      "searchScope": ["Power-BI"],
      "titleSuffix": "Power BI"

    },
    "fileMetadata": {},
    "template": [],
    "dest": "powerbi-docs"
  }
}